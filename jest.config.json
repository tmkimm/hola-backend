    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "babel-jest",
      "^.+\\.mjs$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
